{
  "name": "backend",
  "version": "0.0.0",
  "description": "typescript-express-starter-default",
  "license": "ISC",
  "scripts": {
    "build": "rm -rf lib/* && tsc",
    "start": "NODE_ENV=production ts-node --transpile-only src/server.ts",
    "dev": "NODE_ENV=development nodemon --watch src --delay 1 --exec 'ts-node' src/server.ts",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "orm": "ts-node --project tsconfig.server.json ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "argon2": "^0.25.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cors": "^2.8.5",
    "envalid": "^6.0.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "hpp": "^0.2.3",
    "jest": "^25.1.0",
    "morgan": "^1.9.1",
    "pg": "^7.17.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.6.2",
    "typeorm": "^0.2.22",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/hpp": "^0.2.1",
    "@types/jest": "^25.1.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.5.0",
    "@types/supertest": "^2.0.8",
    "supertest": "^4.0.2",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2"
  }
}
